{
  "env": {
    "commonjs": true,
    "es2021": true,
    "node": true,
    "browser": true,
    "jquery": true
  },
  "extends": ["airbnb-base", "prettier", "plugin:jest/recommended"],
  "parserOptions": {
    "ecmaVersion": 12
  },
  "plugins": ["prettier", "jest"],
  "rules": {
    "prettier/prettier": "error",
    "no-console": "off",
    "object-shorthand": "off",
    "no-else-return": "off",
    "no-underscore-dangle": "off",
    "no-restricted-syntax": "off"
  }
}
